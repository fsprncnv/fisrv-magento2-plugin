{
  "name": "fiserv-ipg/checkout",
  "description": "Official Fisrv Payment Gateway for Magento",
  "type": "magento2-module",
  "version": "1.0.4",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require": {
    "fiserv-ipg/php-client": "1.0.2"
  },
  "autoload": {
    "files": [
      "registration.php"
    ]
  },
  "archive": {
    "exclude": [
      ".gitlab-ci.yml",
      ".gitignore",
      "/*.*",
      "/.*",
      "!/*.php",
      "!composer.json",
      ".idea",
      "tests",
      "vendor",
      "docker"
    ]
  },
  "scripts": {
    "build": [
      "@composer --no-plugins --no-interaction archive"
    ],
    "format": [],
    "format-fix": [
      "../../../../vendor/bin/php-cs-fixer fix"
    ],
    "analyse": [
      "vendor/bin/phpstan analyse ."
    ]
  },
  "scripts-descriptions": {
    "build": "Build project artifact."
  },
  "require-dev": {},
  "config": {
    "allow-plugins": true
  }
}